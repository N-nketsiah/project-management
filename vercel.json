{
  "version": 2,
  "buildCommand": "cd taskflow && npm run build",
  "outputDirectory": "taskflow/dist/taskflow/browser",
  "installCommand": "npm install && cd taskflow && npm install",
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
