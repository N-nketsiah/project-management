{
  "version": 2,
  "builds": [
    {
      "src": "taskflow/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "taskflow/dist/taskflow/browser",
        "buildCommand": "cd ../.. && cd taskflow && npm run build"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
